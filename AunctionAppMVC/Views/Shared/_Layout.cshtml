                <ul>
                    <li><a class="nav-link scrollto " asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>

                    <li><a class="nav-link scrollto " asp-area="" asp-controller="Aunction" asp-action="Index">Aunctions</a></li>

                    <li><a class="nav-link scrollto" asp-area="" asp-controller="Aunction" asp-action="NewAunction">New Aunction</a></li>

                    <li><a class="nav-link scrollto" asp-area=" " asp-controller="Aunction" asp-action="Register">Register</a></li>

                    <li><a class="nav-link scrollto" asp-area="" asp-controller="TodoList" asp-action="New">Aunctions</a></li>
                    <li><a class="nav-link scrollto" asp-area="" asp-controller="TodoList" asp-action="New">Login</a></li>
                    <li><a class="nav-link scrollto" asp-area="" asp-controller="TodoList" asp-action="New">Logout</a></li>


                </ul>

        </div>
